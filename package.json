{
  "name": "eStundnzettl",
  "private": true,
  "version": "6.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "postinstall": "npx replace-in-files-cli --string=\"proguard-android.txt\" --replacement=\"proguard-android-optimize.txt\" \"node_modules/@capacitor/*/android/build.gradle\" \"node_modules/@capgo/*/android/build.gradle\" \"node_modules/@capacitor-community/*/android/build.gradle\"",
    "cap:open": "npx cap open android",
    "version": "node scripts/sync-version.js && git add src/hooks/constants.js android/app/build.gradle",
    "release:patch": "node scripts/confirm-release.js && npm version patch && git push && git push --tags && npm run build && npx cap sync && npx cap open android",
    "release:minor": "node scripts/confirm-release.js && npm version minor && git push && git push --tags && npm run build && npx cap sync && npx cap open android",
    "release:major": "node scripts/confirm-release.js && npm version major && git push && git push --tags && npm run build && npx cap sync && npx cap open android"
  },
  "dependencies": {
    "@capacitor-community/date-picker": "^7.1.0",
    "@capacitor/android": "^8.0.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/core": "^8.0.1",
    "@capacitor/dialog": "^8.0.0",
    "@capacitor/filesystem": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/share": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capgo/capacitor-social-login": "^8.2.13",
    "@tailwindcss/vite": "^4.1.17",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "html2pdf.js": "^0.14.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.1",
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^17.0.0",
    "postcss": "^8.5.6",
    "replace-in-files-cli": "^4.0.0",
    "tailwindcss": "^4.1.17",
    "vite": "^7.2.4"
  }
